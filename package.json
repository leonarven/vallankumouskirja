	"scripts": {
		"watch":  "parcel watch --target default",
		"build":  "parcel build --target default",
		"server": "node_modules/.bin/http-server docs/",
		"serve":  "parcel build --target default && node_modules/.bin/http-server docs/"
	},
	"repository": {
		"type": "git",
