{
	"name": "vallankumouskirja",
	"version": "3.0.1",
	"description": "",
	"targets": {
		"default": {
			"source": "src/index.html",
			"context": "browser",
			"outputFormat": "commonjs",
			"publicUrl": ".",
			"distDir": "./docs",
			"sourceMap": false
		}
