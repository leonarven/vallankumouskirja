	"dependencies": {
		"angular": "^1.7.9",
		"angular-ui-router": "^1.0.30",
		"bootstrap": "^3.3.6",
		"hammerjs": "^2.0.8"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@parcel/packager-raw-url": "^2.7.0",
		"@parcel/resolver-glob": "^2.7.0",
